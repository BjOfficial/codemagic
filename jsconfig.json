{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@assets/*": ["./src/assets/*"],
      "@components/*": ["./src/components/*"],
      "@screens/*": ["./src/screens/*"],
      "@constants/*": ["./src/constants/*"],
      "@shared/*": ["./src/shared/*"],
      "@navigation/*": ["./src/navigation/*"],
      "@utils/*": ["./src/utils/*"],
      "@redux/*": ["./src/redux/*"]
    },
    "noUnusedLocals": true,
    "noUnusedParameters": true
  }
}
